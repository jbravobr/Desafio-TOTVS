services:
  web:
    build:
      context: C:\Users\Rodrigo Amaro\source\repos\Desafio-TOTOVS
      dockerfile: Web/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Web
      target: base
    container_name: Web_1
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages;/root/.nuget/fallbackpackages2
    image: web:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages --additionalProbingPath
        /root/.nuget/fallbackpackages2  "/app/bin/Debug/net5.0/Web.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\Users\Rodrigo Amaro\source\repos\Desafio-TOTOVS\Web:/app:rw
    - C:\Users\Rodrigo Amaro\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Rodrigo Amaro\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Rodrigo Amaro\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Microsoft\Xamarin\NuGet:/root/.nuget/fallbackpackages:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages2:ro
    - C:\Users\Rodrigo Amaro\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\Rodrigo Amaro\source\repos\Desafio-TOTOVS:/src:rw
version: '3.4'